{
    "extends": "airbnb-base",
    "parserOptions": { "ecmaVersion": 2018 },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": "off", // ["error", { "allowAfterThis": true }],
        "no-new": "off",
        "class-methods-use-this": "off",
        "no-unused-expressions": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    }
}