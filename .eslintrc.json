{
    "extends": "airbnb-base",
    "parserOptions": { "ecmaVersion": 2018 },
    "rules": {
        "no-underscore-dangle": "off",
        "no-new": "off",
        "class-methods-use-this": "off",
        "no-unused-expressions": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    }
}